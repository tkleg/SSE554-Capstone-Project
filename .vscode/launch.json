    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "JavaFXTest",
            "request": "launch",
            "mainClass": "org.troy.capstone.JavaFXTest",
            "projectName": "Project",
            "vmArgs": "--module-path \"${workspaceFolder}/target/dependency\" --add-modules javafx.controls,javafx.fxml"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "SwingTest",
            "request": "launch",
            "mainClass": "org.troy.capstone.SwingTest"
        }
    ]
}